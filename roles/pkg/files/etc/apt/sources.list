# deb http://debian.ethz.ch/debian buster main

deb http://debian.ethz.ch/debian buster main non-free contrib
deb-src http://debian.ethz.ch/debian buster main non-free contrib

deb http://security.debian.org/debian-security buster/updates main contrib non-free
deb-src http://security.debian.org/debian-security buster/updates main contrib non-free

# buster-updates, previously known as 'volatile'
deb http://debian.ethz.ch/debian buster-updates main contrib non-free
deb-src http://debian.ethz.ch/debian buster-updates main contrib non-free
